.plist{
	width:70%;height: 100%;padding: 4px;box-sizing: border-box;position: absolute;top: 0px;right:0px;padding-right:0px;
}
.plist .plist_item{
	width:60px;height:60px;line-height:60px;text-align:center;border:1px #999999 solid;cursor:pointer;float:right;margin:0 5px 0 0;
}
.plist .active{
	background: #ffe48d;
}
.plist .plist_item:hover{
	background: #ffe48d;
}

.logo_container{
	width:30%;height:100%;float:left;padding-left:10px;
}
.logo_container .info{
	width:100%;height:20px;font-size:12px;
}
.logo_container .info select{
	font-size:12px;
}
.logo_container .info span{
	margin:0 5px 0 0;
}
.logo{
	width:100%;height:50px;line-height:50px;font-size:22px;
}
.logo img{
	float:left;width:50px;height:50px;
}
.logo span{
	float:left;padding-top:-50px;margin-left:10px;font-weight:bold;
}