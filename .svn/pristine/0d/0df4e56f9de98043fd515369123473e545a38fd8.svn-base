package com.meme.core.pojo;


public class DictItemView extends DictItem{

	private static final long serialVersionUID = -3636635948120951966L;
	private String type;
	private String dictgroupcode;
    private String dictgroupname;
    
    private String isdefaultString;

	public String getIsdefaultString() {
		return isdefaultString;
	}

	public void setIsdefaultString(String isdefaultString) {
		this.isdefaultString = isdefaultString;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getDictgroupcode() {
		return dictgroupcode;
	}

	public void setDictgroupcode(String dictgroupcode) {
		this.dictgroupcode = dictgroupcode;
	}

	public String getDictgroupname() {
		return dictgroupname;
	}

	public void setDictgroupname(String dictgroupname) {
		this.dictgroupname = dictgroupname;
	}

}